<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="samsung.ejemplos.basicwebservice"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="samsung.ejemplos.basicwebservice.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="samsung.ejemplos.basicwebservice.ViewDataActivity" />

        <service
            android:name="samsung.ejemplos.basicwebservice.ConsultaFichasService"
            android:exported="false" />

        <receiver
            android:name="samsung.ejemplos.basicwebservice.ConsultaFichasReceiver"
            android:exported="false" >
            <intent-filter>
                <action android:name="samsung.ejemplos.fichas.action.FIN" />
                <action android:name="samsung.ejemplos.fichas.action.ERROR" />
                <action android:name="samsung.ejemplos.fichas.action.NO_RECORD" />
            </intent-filter>
        </receiver>
    </application>

</manifest>